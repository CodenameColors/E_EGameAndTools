<Window x:Class="DatabaseTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DatabaseTool"
				xmlns:bixbite="clr-namespace:BixBite;assembly=BixBite"
				xmlns:system="clr-namespace:System;assembly=System"
        xmlns:combat="clr-namespace:BixBite.Combat;assembly=BixBite"
        xmlns:combatequip="clr-namespace:BixBite.Combat.Equipables;assembly=BixBite"
        xmlns:item="clr-namespace:BixBite.Items;assembly=BixBite"
        xmlns:system1="clr-namespace:System;assembly=mscorlib"
				Title="Emeralds And Elements Database Entry Tool"
        mc:Ignorable="d" Background="Black"
        Height="1000" Width="1200"
        Loaded="MainWindow_OnLoaded"
        >
	<Window.Resources>
		<ObjectDataProvider x:Key="EJobValues"
		                    MethodName="GetValues" ObjectType="{x:Type system1:Enum}">
			<ObjectDataProvider.MethodParameters>
				<x:Type TypeName="combat:EJob"/>
			</ObjectDataProvider.MethodParameters>
		</ObjectDataProvider>
		<ObjectDataProvider x:Key="EStanceValues" MethodName="GetValues" ObjectType="{x:Type system1:Enum}">
			<ObjectDataProvider.MethodParameters>
				<x:Type TypeName="combat:EStanceType"/>
			</ObjectDataProvider.MethodParameters>
		</ObjectDataProvider>
		<ObjectDataProvider x:Key="ERarityType" MethodName="GetValues" ObjectType="{x:Type system1:Enum}">
			<ObjectDataProvider.MethodParameters>
				<x:Type TypeName="combat:ERarityType"/>
			</ObjectDataProvider.MethodParameters>
		</ObjectDataProvider>
		<ObjectDataProvider x:Key="EWeaponType" MethodName="GetValues" ObjectType="{x:Type system1:Enum}">
			<ObjectDataProvider.MethodParameters>
				<x:Type TypeName="combat:EWeaponType"/>
			</ObjectDataProvider.MethodParameters>
		</ObjectDataProvider>
		<ObjectDataProvider x:Key="EAccessoryType" MethodName="GetValues" ObjectType="{x:Type system1:Enum}">
			<ObjectDataProvider.MethodParameters>
				<x:Type TypeName="combatequip:EAccessoryType"/>
			</ObjectDataProvider.MethodParameters>
		</ObjectDataProvider>
		<ObjectDataProvider x:Key="EClothesType" MethodName="GetValues" ObjectType="{x:Type system1:Enum}">
			<ObjectDataProvider.MethodParameters>
				<x:Type TypeName="combatequip:EClothesType"/>
			</ObjectDataProvider.MethodParameters>
		</ObjectDataProvider>
		<ObjectDataProvider x:Key="EMagicElementType" MethodName="GetValues" ObjectType="{x:Type system1:Enum}">
			<ObjectDataProvider.MethodParameters>
				<x:Type TypeName="combat:EMagicType"/>
			</ObjectDataProvider.MethodParameters>
		</ObjectDataProvider>
		<ObjectDataProvider x:Key="EDamageType" MethodName="GetValues" ObjectType="{x:Type system1:Enum}">
			<ObjectDataProvider.MethodParameters>
				<x:Type TypeName="combat:EDamageModifiers"/>
			</ObjectDataProvider.MethodParameters>
		</ObjectDataProvider>
		<ObjectDataProvider x:Key="EItemType" MethodName="GetValues" ObjectType="{x:Type system1:Enum}">
			<ObjectDataProvider.MethodParameters>
				<x:Type TypeName="item:EItemType"/>
			</ObjectDataProvider.MethodParameters>
		</ObjectDataProvider>
		<ObjectDataProvider x:Key="ESizeType" MethodName="GetValues" ObjectType="{x:Type system1:Enum}">
			<ObjectDataProvider.MethodParameters>
				<x:Type TypeName="combat:ESize"/>
			</ObjectDataProvider.MethodParameters>
		</ObjectDataProvider>
		<ObjectDataProvider x:Key="ERarity" MethodName="GetValues" ObjectType="{x:Type system1:Enum}">
			<ObjectDataProvider.MethodParameters>
				<x:Type TypeName="combat:ERarityType"/>
			</ObjectDataProvider.MethodParameters>
		</ObjectDataProvider>
		<local:EnumToStringConverter x:Key="DefaultEnumToStringConverter"/>
		<local:IntToStanceEnum x:Key="IntToStanceEnumName"/>
	</Window.Resources>
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="30"/>
			<RowDefinition Height="*"/>
			<RowDefinition Height="30"/>
		</Grid.RowDefinitions>

		<TextBlock Grid.Column="0" Grid.Row="0" Text="Selected Database:"  FontWeight="Normal" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" ToolTip="Index 0x1000">
			<TextBlock.Style>
				<Style TargetType="TextBlock">
					<Setter Property="Foreground" Value="{Binding Path=AppTextColor, UpdateSourceTrigger=PropertyChanged ,Mode=TwoWay}"></Setter>
					<Style.Triggers>
						<DataTrigger Binding="{Binding Path=AppBoxFillColor, UpdateSourceTrigger=PropertyChanged ,Mode=TwoWay}" Value="{x:Null}">
							<Setter Property="Foreground" Value="White"></Setter>
						</DataTrigger>
					</Style.Triggers>
				</Style>
			</TextBlock.Style>
		</TextBlock>
		<TextBox x:Name="Databases_TB" Grid.Row="0" Margin="130,0,100,0" Background="Gray" HorizontalAlignment="Stretch" VerticalAlignment="Center">
		</TextBox>
		<Button Content="Browse" Width="50" Margin="320,0,10,0" HorizontalAlignment="Right" VerticalAlignment="Center"
		        Click="BrowseForDataBase_BTN_Click"/>


		<TabControl Background="Transparent" Margin="0,23,20,27" Grid.RowSpan="2">
			<TabItem Header="FollowUpAttacks">
				<Grid>
					<Grid.RowDefinitions>
						<RowDefinition Height="30"/>
						<RowDefinition Height="2"/>
						<RowDefinition Height="*"/>
						<RowDefinition Height="30"/>
					</Grid.RowDefinitions>

					<Button Content="Change to Edit" Width="100" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"
					        Click="ChangeGridToEdit"/>
					<Button Content="Change to Add" Width="100" Margin="160,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"
					        Click="ChangeGridToAdd"/>

					<GridSplitter Grid.Row="1" Grid.ColumnSpan="100" ResizeDirection="Rows" IsEnabled="False" Background="Gray"
					              Margin="-2,0,-2,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
					</GridSplitter>

					<Grid x:Name="AddGrid_FollowupAttacks" Grid.Row="2" Visibility="Hidden">
		
						<Grid>
							<Grid.RowDefinitions>
								<RowDefinition Height="30"/>
								<RowDefinition Height="30"/>
								<RowDefinition Height="30"/>
								<RowDefinition Height="30"/>
								<RowDefinition />
								<RowDefinition Height="30"/>
							</Grid.RowDefinitions>


							<TextBlock Grid.Row="0" Text="CSV File of Match 2s" Foreground="White" Margin="10,5,0,0" 
							           HorizontalAlignment="Left" VerticalAlignment="Top"/>
							<TextBox x:Name="FollowUpAttackCSV_2_TB" Grid.Row="0" Background="Gray" Margin="200,0,70,0" HorizontalAlignment="Stretch" VerticalAlignment="Center"/>
							<Button Content="Browse" Width="50" Margin="320,0,10,0" HorizontalAlignment="Right" VerticalAlignment="Center"
							        Click="BrowseForFollowUpCSV_2_BTN_Click"/>

							<TextBlock Grid.Row="1" Text="CSV File of Match 3s" Foreground="White" Margin="10,5,0,0" 
							           HorizontalAlignment="Left" VerticalAlignment="Top"/>
							<TextBox x:Name="FollowUpAttackCSV_3_TB" Grid.Row="1" Background="Gray" Margin="200,0,70,0" HorizontalAlignment="Stretch" VerticalAlignment="Center"/>
							<Button Grid.Row="1" Content="Browse" Width="50" Margin="320,0,10,0" HorizontalAlignment="Right" VerticalAlignment="Center"
							        Click="BrowseForFollowUpCSV_3_BTN_Click"/>

							<TextBlock Grid.Row="2" Text="CSV File of Match 4s." Foreground="White" Margin="10,5,0,0" 
							           HorizontalAlignment="Left" VerticalAlignment="Top"/>
							<TextBox x:Name="FollowUpAttackCSV_4_TB" Grid.Row="2" Background="Gray" Margin="200,0,70,0" HorizontalAlignment="Stretch" VerticalAlignment="Center"/>
							<Button Grid.Row="2" Content="Browse" Width="50" Margin="320,0,10,0" HorizontalAlignment="Right" VerticalAlignment="Center"
							        Click="BrowseForFollowUpCSV_4_BTN_Click"/>

							<Button Grid.Row="3" Content="Clear" Width="50" Margin="320,0,80,0" HorizontalAlignment="Right" VerticalAlignment="Center"
							        Click="ClearFollowUps_BTN_Click"/>
							<Button Grid.Row="3" Content="Run" Width="50" Margin="320,0,10,0" HorizontalAlignment="Right" VerticalAlignment="Center"
							        Click="RunForFollowUpCSV_BTN_Click"/>

							<ListBox x:Name="FollowUpAttacks_LB" Grid.Row="4" BorderThickness="2" BorderBrush="AliceBlue" ItemsSource="{Binding CurrentFollowUpAttacks}" Background="Transparent" Margin="10,10,10,10">
								<ListBox.ItemTemplate>
									<DataTemplate>
										<Grid>
												<Grid.ColumnDefinitions>
													<ColumnDefinition Width="150"/>
													<ColumnDefinition Width="10"/>
													<ColumnDefinition Width="150"/>
													<ColumnDefinition Width="10"/>
													<ColumnDefinition Width="150"/>
													<ColumnDefinition Width="10"/>
													<ColumnDefinition Width="150"/>
												</Grid.ColumnDefinitions>

											<TextBlock Grid.Column="0" Foreground="White" Text="{Binding .Job_1}" ></TextBlock>
											<TextBlock Grid.Column="2" Foreground="White" Text="{Binding  .Job_2}" ></TextBlock>
											<TextBlock Grid.Column="4" Foreground="White" Text="{Binding .Job_3}" ></TextBlock>
											<TextBlock Grid.Column="6" Foreground="White" Text="{Binding .Job_4}" ></TextBlock>

											<GridSplitter Grid.Column="1" Grid.ColumnSpan="1" Width="2" ResizeDirection="Columns" IsEnabled="False" Margin="-2,0,10,0" HorizontalAlignment="Right" VerticalAlignment="Stretch">
												</GridSplitter>
												<GridSplitter Grid.Column="3" Grid.ColumnSpan="1" Width="2" ResizeDirection="Columns" IsEnabled="False" Margin="-2,0,10,0" HorizontalAlignment="Right" VerticalAlignment="Stretch">
												</GridSplitter>
												<GridSplitter Grid.Column="5" Grid.ColumnSpan="1" Width="2" ResizeDirection="Columns" IsEnabled="False" Margin="-2,0,10,0" HorizontalAlignment="Right" VerticalAlignment="Stretch">
											</GridSplitter>

											</Grid>
									</DataTemplate>
								</ListBox.ItemTemplate>
							</ListBox>


						</Grid>
					</Grid>

					<Grid x:Name="EditGrid_FollowupAttacks" Grid.Row="2" Visibility="Visible">

						<Grid>
							<Grid.RowDefinitions>
								<RowDefinition/>
							</Grid.RowDefinitions>


							<ListBox x:Name="EditFollowUpAttacks_LB" Grid.Row="4" BorderThickness="2" BorderBrush="AliceBlue" ItemsSource="{Binding CurrentFollowUpAttacks}" Background="Transparent" Margin="10,10,10,10">
								<ListBox.ItemTemplate>
									<DataTemplate>
										<Grid>
											<Grid.ColumnDefinitions>
												<ColumnDefinition Width="150"/>
												<ColumnDefinition Width="10"/>
												<ColumnDefinition Width="150"/>
												<ColumnDefinition Width="10"/>
												<ColumnDefinition Width="150"/>
												<ColumnDefinition Width="10"/>
												<ColumnDefinition Width="150"/>
												<ColumnDefinition Width="10"/>
												<ColumnDefinition Width="150"/>
												<ColumnDefinition Width="10"/>
												<ColumnDefinition Width="150"/>
											</Grid.ColumnDefinitions>

											<TextBlock Grid.Column="0" Foreground="White" Text="{Binding .Job_1}" ></TextBlock>
											<TextBlock Grid.Column="2" Foreground="White" Text="{Binding  .Job_2}" ></TextBlock>
											<TextBlock Grid.Column="4" Foreground="White" Text="{Binding .Job_3}" ></TextBlock>
											<TextBlock Grid.Column="6" Foreground="White" Text="{Binding .Job_4}" ></TextBlock>
											<TextBox Grid.Column="8" Foreground="Black" Text="{Binding .Name, Mode=TwoWay }"/>
											<TextBox Grid.Column="10" Foreground="Black" Text="{Binding .Function_PTR, Mode=TwoWay }"/>

											<GridSplitter Grid.Column="1" Grid.ColumnSpan="1" Width="2" ResizeDirection="Columns" IsEnabled="False" Margin="-2,0,10,0" HorizontalAlignment="Right" VerticalAlignment="Stretch">
											</GridSplitter>
											<GridSplitter Grid.Column="3" Grid.ColumnSpan="1" Width="2" ResizeDirection="Columns" IsEnabled="False" Margin="-2,0,10,0" HorizontalAlignment="Right" VerticalAlignment="Stretch">
											</GridSplitter>
											<GridSplitter Grid.Column="5" Grid.ColumnSpan="1" Width="2" ResizeDirection="Columns" IsEnabled="False" Margin="-2,0,10,0" HorizontalAlignment="Right" VerticalAlignment="Stretch">
											</GridSplitter>
											<GridSplitter Grid.Column="7" Grid.ColumnSpan="1" Width="2" ResizeDirection="Columns" IsEnabled="False" Margin="-2,0,10,0" HorizontalAlignment="Right" VerticalAlignment="Stretch">
											</GridSplitter>
											<GridSplitter Grid.Column="9" Grid.ColumnSpan="1" Width="2" ResizeDirection="Columns" IsEnabled="False" Margin="-2,0,10,0" HorizontalAlignment="Right" VerticalAlignment="Stretch">
											</GridSplitter>

										</Grid>
									</DataTemplate>
								</ListBox.ItemTemplate>
							</ListBox>
						</Grid>

					</Grid>

					<Button Grid.Row="3" Content="Update Database!"  Margin="50,0,50,0" HorizontalAlignment="Stretch" VerticalAlignment="Center"
					        Click="EditFollowUpAttacksInDB_BTN_Click"/>
				</Grid>
			</TabItem>
			<TabItem Header="Jobs">
				<Grid>
					<Grid.RowDefinitions>
						<RowDefinition Height="30"/>
						<RowDefinition Height="2"/>
						<RowDefinition Height="*"/>
						<RowDefinition Height="30"/>
					</Grid.RowDefinitions>

					<Button Content="Change to Edit" Width="100" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"
					        Click="ChangeGridToEdit"/>
					<Button Content="Change to Add" Width="100" Margin="160,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"
					        Click="ChangeGridToAdd"/>

					<GridSplitter Grid.Row="1" Grid.ColumnSpan="100" ResizeDirection="Rows" IsEnabled="False" Background="Gray"
					              Margin="-2,0,-2,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
					</GridSplitter>

					<Grid x:Name="AddGrid_Jobs" Grid.Row="2" Visibility="Visible">
						<Grid.RowDefinitions>
							<RowDefinition Height="30"/>
							<RowDefinition Height="30"/>
							<RowDefinition Height="30"/>
						</Grid.RowDefinitions>
						<StackPanel Orientation="Horizontal" Margin="10,10,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" >
							<TextBlock Text="Job Name:" Foreground="White" Padding="0,0,0,0"/>
							<ComboBox x:Name="JobName_CB" Width="200" Background="DimGray" ItemsSource="{Binding Source={StaticResource EJobValues}}"/>
						</StackPanel>

						<StackPanel Orientation="Horizontal" Margin="300,10,10,0" HorizontalAlignment="Left" VerticalAlignment="Top"  >
							<TextBlock Text="Stance Left:" Foreground="White" Padding="0,0,10,0"/>
							<ComboBox x:Name="StanceLeft_CB" Width="100" Background="DimGray"  ItemsSource="{Binding Source={StaticResource EStanceValues}}"/>
						</StackPanel>
						<StackPanel Orientation="Horizontal" Margin="500,10,10,0" HorizontalAlignment="Left" VerticalAlignment="Top" >
							<TextBlock Text="Stance Middle:" Foreground="White" Padding="0,0,10,0"/>
							<ComboBox x:Name="StanceMiddle_CB" Width="100" Background="DimGray"  ItemsSource="{Binding Source={StaticResource EStanceValues}}"/>
						</StackPanel>
						<Button Content="Add to DB" Margin="700,10,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"
						        Click="AddJobToDB_BTN_OnClick"/>
						<TextBlock Grid.Row="1" Text="Status:" Foreground="White" Margin="5,5,400,5"></TextBlock>
						<TextBlock x:Name="AddToJobsDB_TB" Grid.Row="1" Background="Gray" Foreground="White" Margin="66,5,407,5"></TextBlock>
					</Grid>
					<Grid x:Name="EditGrid_Jobs" Grid.Row="2" Visibility="Hidden">
						<!--<TextBlock Text="And you have no life" Foreground="White"/>-->
						<Grid.RowDefinitions>
							<RowDefinition Height="30"/>
							<RowDefinition Height="2"/>
							<RowDefinition Height="*"/>
						</Grid.RowDefinitions>

						<TextBlock Text="Job Name" Foreground="White" Width="200" FontWeight="Bold" Margin="15,0,0,0" HorizontalAlignment="Left"/>
						<TextBlock Text="Left Stance" Foreground="White" Width="200" FontWeight="Bold" Margin="215,0,0,0" HorizontalAlignment="Left"/>
						<TextBlock Text="Right Stance" Foreground="White" Width="200" FontWeight="Bold" Margin="415,0,0,0" HorizontalAlignment="Left"/>
						<ListBox x:Name="EditJobsDB_LB" Grid.Row="2" ItemsSource="{Binding CurrentJobsInDatabase}" Background="Transparent" Margin="10,10,10,0" BorderThickness="1">
							<ListBox.ItemTemplate>
								<DataTemplate>
									<Grid Margin="0,0,0,5">
										<Grid.ColumnDefinitions>
											<ColumnDefinition Width="200" />
											<ColumnDefinition Width="200" />
											<ColumnDefinition Width="200" />
											<ColumnDefinition Width="200" />
										</Grid.ColumnDefinitions>

										<TextBlock Grid.Column="0" Text="{Binding Path=Name}"
								           FontWeight="Normal" FontSize="12" Margin="0,0,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Center">
											<TextBlock.Style>
												<Style TargetType="TextBlock">
													<Setter Property="Foreground" Value="{Binding Path=AppTextColor, UpdateSourceTrigger=PropertyChanged ,Mode=TwoWay}"></Setter>
													<Style.Triggers>
														<DataTrigger Binding="{Binding Path=AppBoxFillColor, UpdateSourceTrigger=PropertyChanged ,Mode=TwoWay}" Value="{x:Null}">
															<Setter Property="Foreground" Value="White"></Setter>
														</DataTrigger>
													</Style.Triggers>
												</Style>
											</TextBlock.Style>
										</TextBlock>
										<TextBlock Grid.Column="1" Text="{Binding Path=Left_Stance, Converter={StaticResource IntToStanceEnumName}}"
											           FontWeight="Normal" FontSize="12" Margin="0,0,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Center">
											<TextBlock.Style>
												<Style TargetType="TextBlock">
													<Setter Property="Foreground" Value="{Binding Path=AppTextColor, UpdateSourceTrigger=PropertyChanged ,Mode=TwoWay}"></Setter>
													<Style.Triggers>
														<DataTrigger Binding="{Binding Path=AppBoxFillColor, UpdateSourceTrigger=PropertyChanged ,Mode=TwoWay}" Value="{x:Null}">
															<Setter Property="Foreground" Value="White"></Setter>
														</DataTrigger>
													</Style.Triggers>
												</Style>
											</TextBlock.Style>
										</TextBlock>
										<TextBlock Grid.Column="2" Text="{Binding Path=Right_Stance, Converter={StaticResource IntToStanceEnumName}}"
											           FontWeight="Normal" FontSize="12" Margin="0,0,0,0" HorizontalAlignment="Stretch" VerticalAlignment="Center">
											<TextBlock.Style>
												<Style TargetType="TextBlock">
													<Setter Property="Foreground" Value="{Binding Path=AppTextColor, UpdateSourceTrigger=PropertyChanged ,Mode=TwoWay}"></Setter>
													<Style.Triggers>
														<DataTrigger Binding="{Binding Path=AppBoxFillColor, UpdateSourceTrigger=PropertyChanged ,Mode=TwoWay}" Value="{x:Null}">
															<Setter Property="Foreground" Value="White"></Setter>
														</DataTrigger>
													</Style.Triggers>
												</Style>
											</TextBlock.Style>
										</TextBlock>
									</Grid>
								</DataTemplate>
							</ListBox.ItemTemplate>
						</ListBox>
						<GridSplitter Grid.Row="1" Grid.ColumnSpan="100" ResizeDirection="Rows" IsEnabled="False" Background="Gray"
						              Margin="-2,0,-2,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
						</GridSplitter>
					</Grid>

				</Grid>
			</TabItem>
			<TabItem Header="Skills">
				<Grid>
					<Grid.RowDefinitions>
						<RowDefinition Height="30"/>
						<RowDefinition Height="2"/>
						<RowDefinition Height="*"/>
						<RowDefinition Height="30"/>
					</Grid.RowDefinitions>

					<Button Content="Change to Edit" Width="100" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"
	                Click="ChangeGridToEdit"/>
					<Button Content="Change to Add" Width="100" Margin="160,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"
	                Click="ChangeGridToAdd"/>

					<Grid Grid.Row="2" x:Name="Skills_Add_Grid" Visibility="Hidden">
						<Grid.RowDefinitions>
							<RowDefinition Height="30"/>
							<RowDefinition Height="30"/>
							<RowDefinition Height="30"/>
							<RowDefinition Height="30"/>
							<RowDefinition Height="30"/>
							<RowDefinition Height="30"/>
							<RowDefinition Height="30"/>
							<RowDefinition Height="*"/>
						</Grid.RowDefinitions>
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="2*"/>
							<ColumnDefinition Width="1*"/>
							<ColumnDefinition Width="1*"/>
						</Grid.ColumnDefinitions>

						<TextBlock Grid.Row="0" Text="Name:" Foreground="White" Margin="5,5,400,5" HorizontalAlignment="Left"/>
						<TextBox x:Name="SkillsName_Add_TB" Grid.Row="0" Width="300" Background="DimGray" Foreground="White" Margin="100,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"/>
						<Path Grid.Row="0" Grid.RowSpan="2" Grid.ColumnSpan="3" Margin="400,0,0,0" Stroke="White" Data="M 0 15 L 470 15 M 460 5 L 470 15 M 460 25 L 470 15" HorizontalAlignment="Left" VerticalAlignment="Top" />

						<TextBlock Grid.Row="1" Text="Physical = Check | Magic = Unchecked:" Foreground="White" Margin="5,5,0,5" HorizontalAlignment="Left"/>
						<CheckBox x:Name="SkillIsPhysical_Add_CB" Grid.Row="1" Width="20" Background="DimGray" Foreground="White" Margin="230,0,0,8" HorizontalAlignment="Left" VerticalAlignment="Bottom"/>

						<!--1.0.0.2v-->
						<TextBlock Grid.Row="1" Text="AoE Width:" Foreground="White" Margin="260,7,0,5" HorizontalAlignment="Left"/>
						<TextBox x:Name="SkillAoEWidth_Add_TB" Grid.Row="1" Text="0" Width="70" Background="DimGray" Foreground="White" Margin="325,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"/>

						<TextBlock Grid.Row="2" Text="Damage = Check | Healing = Unchecked" Foreground="White" Margin="5,5,0,5" HorizontalAlignment="Left"/>
						<CheckBox x:Name="SkillIsDamage_Add_CB" Grid.Row="2" Width="20" Background="DimGray" Foreground="White" Margin="230,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"/>

						<!--1.0.0.2v-->
						<TextBlock Grid.Row="2" Text="AoE Height:" Foreground="White" Margin="260,7,0,5" HorizontalAlignment="Left"/>
						<TextBox x:Name="SkillAoEHeight_Add_TB" Grid.Row="2" Text="0" Width="70" Background="DimGray" Foreground="White" Margin="325,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"/>

						<TextBlock Grid.Row="3" Text="Inflicting Multiplier (x.xxx):" Foreground="White" Margin="5,5,400,5" HorizontalAlignment="Left"/>
						<TextBox x:Name="SkillDamageMultiplier_Add_TB" Grid.Row="3" Width="50" Background="DimGray" Foreground="White" Margin="150,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"/>

						<!--1.0.0.2v-->
						<TextBlock Grid.Row="3" Text="Target Allies?" Foreground="White" Margin="260,5,0,5" HorizontalAlignment="Left"/>
						<CheckBox x:Name="SkillAllies_Add_CB" Grid.Row="3" Width="20" Background="DimGray" Foreground="White" Margin="370,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"/>

						<TextBlock Grid.Row="4" Text="Skill Allowed Weapon Type:" Foreground="White" Margin="5,5,400,5" HorizontalAlignment="Left"/>
						<ComboBox x:Name="SkillWeaponType_Add_CB" Grid.Row="4" Width="250" Background="DimGray" Foreground="Black" Margin="150,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"
						          ItemsSource="{Binding Source={StaticResource EWeaponType}}"/>

						<TextBlock Grid.Row="0" Grid.Column="2" Text="All Current For Mods:" Foreground="White" Margin="5,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"/>
						<TextBox x:Name="SkillModName2_Add_TB" 
						         Text="{Binding Path=Text, ElementName=SkillsName_Add_TB, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
						         Grid.Row="0" Grid.Column="2" IsReadOnly="True" Width="175" Background="DimGray" Foreground="White" Margin="110,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"/>
						<ScrollViewer Grid.Column="1" Grid.ColumnSpan="2"  Grid.Row="1" Grid.RowSpan="6" Margin="5" Background="Gray">
							<ItemsControl x:Name="SkillAllMods_Add_IC" Margin="5" Background="Gray"
						              ItemsSource="{Binding SkillsCurrentLinkedModifiers_Add_AllMods }">
								<ItemsControl.ItemTemplate>
									<DataTemplate>
										<Grid>
											<Grid.ColumnDefinitions>
												<ColumnDefinition Width="150"/>
												<ColumnDefinition Width="10"/>
												<ColumnDefinition Width="*"/>
											</Grid.ColumnDefinitions>

											<TextBlock Grid.Column="0" Foreground="White" Text="{Binding Path=Item1}"></TextBlock>
											<TextBlock Grid.Column="2" Foreground="White" Text="{Binding Path=Item2}"></TextBlock>
